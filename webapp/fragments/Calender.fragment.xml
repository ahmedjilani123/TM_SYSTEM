<core:FragmentDefinition xmlns:unified="sap.ui.unified"
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Popover
		id="myPopover"
		title="Calender"
		class="sapUiResponsivePadding--header sapUiResponsivePadding--footer"
		placement="Bottom"
		initialFocus="email">
		<footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button
					id="email"
					text="OK"
					press="handleCancelPress" />
			</OverflowToolbar>
		</footer>
		<unified:Calendar ></unified:Calendar>
	</Popover>
</core:FragmentDefinition>
